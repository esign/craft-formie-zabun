{
  "name": "esign/craft-formie-zabun-integration",
  "description": "Zabun integration that extends Formie Craft CMS plugin.",
  "type": "craft-plugin",
  "license": "mit",
  "support": {
    "email": "support@esign.eu",
    "issues": "https://github.com/esign/craft-formie-zabun-integration/issues?state=open",
    "source": "https://github.com/esign/craft-formie-zabun-integration",
    "docs": "https://github.com/esign/craft-formie-zabun-integration",
    "rss": "https://github.com/esign/craft-formie-zabun-integration/releases.atom"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.0.0",
    "verbb/formie": "^3.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "esign\\craftformiezabunintegration\\": "src/"
    }
  },
  "extra": {
    "handle": "formie-zabun",
    "name": "Formie Zabun Integration",
    "developer": "dieter.vanhove@dynamate.be",
    "documentationUrl": "https://github.com/esign/craft-formie-zabun-integration"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}